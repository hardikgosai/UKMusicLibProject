@model ContractViewModel
@using UKMusicLibProject.Models

<section id="Portfolio" class="content">

	<!-- Container -->
	<div class="container portfolio_title">

		<!-- Title -->
		<div class="section-title">
			<h2>Add Contract </h2>

		</div>
		<!--/Title -->

	</div>

	<div class="portfolio-top"></div>

	<!-- Portfolio Filters -->
	<div class="portfolio">

		<div id="filters" class="sixteen columns">
			<ul class="clearfix">

				<li>

					<a id="Concert" asp-area="" asp-action="Index">
						<h5>List Contract</h5>
					</a>
				</li>

			</ul>
		</div>
	</div>
</section>
<section id="aboutUs">
	<!--Aboutus-->
	<div class="inner_wrapper about-us aboutUs-container fadeInLeft animated wow">
		<div class="container">

<form asp-action="AddContract">
	<div class="row">
		
			<div class="form-group">
				<label asp-for="ContractType"></label>
				<input asp-for="ContractType" class="form-control" />
				<span asp-validation-for="ContractType" class="text-danger"></span>
			</div>
			

			<div class="form-group">
				<label asp-for="ContractWith"></label>
				<input asp-for="ContractWith" class="form-control" />
				<span asp-validation-for="ContractWith" class="text-danger"></span>

			</div>
			<div class="form-group">
				<label asp-for="StartDate"></label>
				<input type="date" asp-for="StartDate" class="form-control" />
				<span asp-validation-for="StartDate" class="text-danger"></span>
			</div>

			<div class="form-group">
				<label asp-for="EndDate"></label>
				<input type="date" asp-for="EndDate" class="form-control" />
				<span asp-validation-for="EndDate" class="text-danger"></span>
			</div>
		

		<div class="form-group">
			<label asp-for="Revenue"></label>
			<input asp-for="Revenue" class="form-control" />
			<span asp-validation-for="Revenue" class="text-danger"></span>

		</div>
		<div class="form-group">
			<label asp-for="Status"></label>
			<input asp-for="Status" class="form-control" />
			<span asp-validation-for="Status" class="text-danger"></span>
		</div>
		

		<br /><br /><br />
		<div class="form-group">
			<input type="submit" value="Create" class="btn btn-primary" />
		</div>
	</div>
</form>
</div>
</div>
</section>
